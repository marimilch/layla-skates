-- game loop
function _update()
	if 
		nxtphase 
		and ttrans>=10 
	then
		phase=nxtphase
		nxtphase=nil
		phases[phase].initfn()
	end
 get_phase().updtfn()
end
function _draw()
 get_phase().drawfn()
 if phaset then
 	tdraw()
 end
end
function _init()
	init_phases()
	tglob=0
	phase='nul'
	set_phase('game')
end
