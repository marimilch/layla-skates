function handle_crts()
	for c in all(crts) do
		if (
			costatus(c)=="suspended"
		) 
		then
			local st,msg=coresume(c)
			if (not st) then
				cls()
				printml(msg)
				stop()
			end
		end
		if (
			costatus(c)=="dead"
		) then
			del(crts,c)
		end
	end
end
function printml(s)
	for i=1,flr(#s/32) do
		print(
			sub(s,(i-1)*32,min(i*32,#s)),
			0,
			(i-1)*7
		)
	end
end
function add_crt(f,p1,p2,p3)
	local f_=function()
		return f(p1,p2,p3)
	end
	add(
		crts,
		cocreate(f_)
	)
end
function waitco(t)
	for i=0,t do
		yield()
	end
end
