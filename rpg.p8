-- game loop
function _update()
	if 
		nxtphase 
		and ttrans>=10 
	then
		phase=nxtphase
		nxtphase=nil
		ondark()
		phases[phase].initfn()
	end
 get_phase().updtfn()
end
function _draw()
 get_phase().drawfn()
 if phaset then
 	tdraw()
 end
end
