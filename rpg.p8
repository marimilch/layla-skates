function hndl_mvcnt(s)
	if (
		not s.ismvn
		or s.static
	) then
		return
	end
	s.mvcnt+=1
 s.mvcnt%=8
 if (s.mvcnt==0) then
 	s.frame+=1
 	s.frame%=2
 	mvsfx(s)
 end
 s.ismvn=false
end
function mvsfx(s)
	local blw=mget(
		lvl.xt+flr((s.left+4)/8),
		lvl.yt+flr((s.top+4)/8)
	)
	if blw==67 or blw==68 then
		sfx(12, 1)
	else
		sfx(1, 1)
	end
end
function move_player()
	if (canmove) then
	 if (btn(0)) then
	 	plyr.left-=1 --left
	 	plyr.dir=2
	 	plyr.ismvn=true
	 end
	 if (btn(1)) then
	  plyr.left+=1 --right
	 	plyr.dir=3
	 	plyr.ismvn=true
	 end
	 if (btn(2)) then 
	 	plyr.top-=1 --up
	 	plyr.dir=1
	 	plyr.ismvn=true
	 end
	 if (btn(3)) then 
	 	plyr.top+=1 --down
	 	plyr.dir=0
	 	plyr.ismvn=true
	 end
 	hndl_mvcnt(plyr)
 end
end
