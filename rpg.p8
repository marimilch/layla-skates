function dialog(s, txt)
	add_crt(dialogco,s,txt)
end
function dialogco(s,txt)
	local dlgt=dialogt(txt)
	for t=0,dlgt+30 do
		local startx=s.left-#txt*2
		local startx_=min(
			max(startx,2),
			128-#txt*4-6
		)
		local y_=s.top-13
		for i=0,#txt-1 do
			local si=130
			local t_=t
			if(i==0) si=128
			if(i==#txt-1) si=129
			if(
				t_<7 
				or t_>dlgt
			) then
				si=132
			end
			spr(si,startx_+i*4,y_)
		end
		for i=0,#txt-1 do
			local t_=t-i
			print(
				sub(txt,i+1,i+1),
				startx_+2+i*4,
				y_+1+letterd(t_),
				letterc(t_,txt)
			)
		end
		local si=131
		if(
			t<7 
			or t>dlgt*2+7
		) then
			si=133
		end
		spr(si,startx+#txt*2,y_+7)
		yield()
	end
end
function letterd(t)
	return (
		sin(t/10)
		*max(-.05*t+1,0)
		*2
	)
end
function dialogt(s)
	return #s*5
end
function letterc(t,s)
	local len=dialogt(s)
	if t<2 then
		return 1
	elseif t<4 then
		return 13
	elseif t<len+4 then
		return 6
	elseif t<len+6 then
		return 13
	else
		return 1
	end
end
