function flippyani(si)
	return function(self)
			self.frame+=1
			self.frame%=14
			if self.frame<7 then
				self.sprp=si
			else
				self.sprp=si+1
			end
		end
end
function btno(i)
	local res=false
	if not btn(i) then
		btnos[i]=false
	end
	if
		not btnos[i]
		and btn(i)
 then
 	btnos[i]=true
		res=true
	end
	return res
end
function _if(bool, func1, func2) 
 if bool then return func1() else return func2() end
